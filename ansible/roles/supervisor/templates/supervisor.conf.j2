[program:celery]

command={{ virtualenv }}/bin/paster --plugin=ckanext-archiver celeryd2 run all --config={{ ckan_ini }}
user={{ www_user }}
numprocs=1
stdout_logfile={{ supervisor_log_path }}/celeryd.log
stderr_logfile={{ supervisor_log_path }}/celeryd.log
autostart=true
autorestart=true
startsecs=10
stopwaitsecs = 600
priority=998