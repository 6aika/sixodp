<!-- Matomo -->
<script>
    var _paq = window._paq = window._paq || [];
    /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
    _paq.push(["setExcludedQueryParams", ["fbclid","time"]]);
    _paq.push(['trackPageView']);
    _paq.push(['enableLinkTracking']);
    (function() {
        var u="{{ matomo.domain }}";
        _paq.push(['setTrackerUrl', u+'{{matomo.tracker_filename}}']);
        _paq.push(['setSiteId', '{{ matomo.site_id }}']);
        var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
        g.async=true; g.src='{{ matomo.script_domain }}' + '{{matomo.script_filename}}'; s.parentNode.insertBefore(g,s);
    })();
</script>
<!-- End Matomo Code -->