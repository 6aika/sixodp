    "build-sass": "node-sass ./src/scss/main.scss ckanext/sixodp_theme/fanstatic/sixodp_theme.css",
    "dev": "npm-run-all --parallel watch-sass watch-js",
    "postinstall": "npm-run-all --parallel build"
  },
